{
  "name": "what-to-use-server",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "dev": "nodemon src/index.js --exec babel-node",
    "clean": "rm -rf build && mkdir build",
    "build": "NODE_ENV=production npm run clean && babel -d ./build ./src -s",
    "start": "npm run start:deamon && npm run logs",
    "start:single": "node build/index.js",
    "start:deamon": "pm2 start build/index.js",
    "stop": "pm2 stop build/index.js",
    "logs": "pm2 logs"
  },
  "main": "src/index.js",
  "author": "",
  "license": "ISC",
  "engines": {
    "node": "7.x"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.2.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "body-parser": "^1.17.2",
    "cors": "^2.8.4",
    "express": "^4.15.3",
    "github-api": "^3.0.0",
    "global": "^4.3.2",
    "graphql": "^0.10.3",
    "graphql-server-express": "^1.0.0",
    "graphql-subscriptions": "^0.4.4",
    "graphql-tools": "^1.1.0",
    "lodash": "^4.17.4",
    "lru-cache": "^4.1.1",
    "nedb": "^1.8.0",
    "pm2": "^2.6.1"
  }
}
